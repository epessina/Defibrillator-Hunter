<?xml version='1.0' encoding='utf-8'?>
<widget id="polimi.epessina.defibrillatorhunter" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DefibrillatorHunter</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="edoardo2.pessina@mail.polimi.it" href="http://cordova.io">
        Edoardo Pessina
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="portrait" />
</widget>
